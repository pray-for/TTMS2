<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="SlideMenu">
        <attr name="menu_layout" format="reference"></attr>
        <attr name="content_layout" format="reference"></attr>
        <attr name="right_padding" format="dimension"></attr>
        <attr name="left_slide" format="dimension"></attr>
    </declare-styleable>
    <declare-styleable name="PullListView">
        <attr name="getMoreType">
            <enum name="auto" value="0" />
            <enum name="click" value="1" />
        </attr>
        <attr name="addPullHeaderByUser" format="boolean" />
    </declare-styleable>
    <declare-styleable name="CircleImageView">
        <attr name="civ_border_width" format="dimension" />
        <attr name="civ_border_color" format="color" />
        <attr name="civ_border_overlay" format="boolean" />
        <attr name="civ_fill_color" format="color" />
    </declare-styleable>

    <declare-styleable name="GridChartView">
        <attr name="xLineColor" format="color"/>
        <attr name="xTextColor" format="color"/>
        <attr name="yLineColor" format="color"/>
        <attr name="yTextColor" format="color"/>
        <attr name="gridLineColor" format="color"/>
        <attr name="centerLineColor" format="color"/>
        <attr name="xLineSize" format="dimension"/>
        <attr name="xTextSize" format="dimension"/>
        <attr name="yLineSize" format="dimension"/>
        <attr name="yTextSize" format="dimension"/>
        <attr name="yTextMarginRight" format="dimension"/>
        <attr name="xTextMarginTop" format="dimension"/>
        <attr name="gridLineSize" format="dimension"/>
        <attr name="centerLineSize" format="dimension"/>
        <attr name="centerMarginTop" format="dimension"/>
        <attr name="xItemWidth" format="dimension"/>
        <attr name="maxValue" format="integer" />
        <attr name="unitValue" format="integer" />
        <attr name="startAutoMovingMarginRight" format="dimension"/>
    </declare-styleable>

</resources>
